eureka:
    client:
        fetch-registry: true
        instance:
            hostname: localhost
        register-with-eureka: true
        service-url:
            defaultZone: http://localhost:8761/eureka/
server:
    port: 7070
spring:
    application:
        name: ServerConfig
    cloud:      config:        server:          git:            uri: https://github.com/AbhiramMachiraju/MicroServiceBasic.git
            search-paths:            - ${application}
